{
  "name": "sveltekit-cloudflare-d1",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "wrangler pages dev .svelte-kit/cloudflare",
    "deploy": "wrangler pages deploy .svelte-kit/cloudflare",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply DB --remote",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "db:seed": "npx tsx scripts/seed.ts",
    "db:seed:local": "wrangler d1 execute DB --local --file=./drizzle/seed.sql",
    "db:studio": "drizzle-kit studio",
    "db:refresh:local": "rm -rf .wrangler/state/d1 && wrangler d1 migrations apply DB --local",
    "cf:typegen": "wrangler types"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^4.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.20.0",
    "postcss": "^8.4.32",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "wrangler": "^4.61.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/s3-request-presigner": "^3.980.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "arctic": "^3.7.0",
    "drizzle-orm": "^0.29.0",
    "lucia": "^3.2.2",
    "lucide-svelte": "^0.294.0",
    "resend": "^6.9.1",
    "zod": "^3.22.4"
  },
  "type": "module"
}
