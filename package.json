{
  "name": "sveltekit-cloudflare-d1",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "wrangler pages dev .svelte-kit/cloudflare",
    "deploy": "wrangler pages deploy .svelte-kit/cloudflare",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply DB --remote",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "db:seed": "npx tsx scripts/seed.ts",
    "db:seed:local": "wrangler d1 execute DB --local --file=./drizzle/seed.sql",
    "db:studio": "drizzle-kit studio",
    "db:refresh:local": "rm -rf .wrangler/state && wrangler d1 migrations apply DB --local",
    "cf:typegen": "wrangler types"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.6",
    "@sveltejs/kit": "^2.20.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.0.0",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.0",
    "postcss": "^8.5.0",
    "svelte": "^5.20.0",
    "svelte-check": "^4.2.0",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.0",
    "typescript": "^5.7.0",
    "vite": "^6.2.0",
    "wrangler": "^4.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/s3-request-presigner": "^3.750.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "arctic": "^3.7.0",
    "drizzle-orm": "^0.40.0",
    "lucia": "^3.2.2",
    "lucide-svelte": "^0.475.0",
    "resend": "^4.0.0",
    "zod": "^4.3.6"
  },
  "type": "module"
}
